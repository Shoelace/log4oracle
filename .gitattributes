# end-of-line normalisation
* text=auto

# No Git attributes
.gitattributes export-ignore
.gitignore export-ignore
.gitkeep export-ignore

#sql language definistion
# see https://github.com/github/linguist/issues/1044
*.sql linguist-detectable=true
